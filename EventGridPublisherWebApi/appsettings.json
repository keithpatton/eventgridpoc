{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",


  "EventGridPublishing": {
    "NamespaceEndpoint": "https://eosdomaineventsbus-poc.australiaeast-1.eventgrid.azure.net/",
    "EventBatchSize": 250,
    "PublishingInterval": "00:00:03",
    "Topics": [
      {
        "Name": "customisation",
        "Key": "{SECRET}"
      }
    ]
  },

  "ConnectionStrings": {
    "SqlServer": "{SECRET}",
    "SqlServerLocal": "Server=localhost;Database=wolverine;Integrated Security=True;TrustServerCertificate=True"
  }

}
